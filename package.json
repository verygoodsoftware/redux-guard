{
  "name": "stately",
  "version": "1.0.0",
  "description": "Redux + state management.",
  "main": "dist/index.js",
  "scripts": {
    "precompile": "npm run lint",
    "compile": "tsc",
    "lint": "tslint -c tslint.json 'src/**/*ts'",
    "prestartDev": "npm run lint",
    "startDev": "ts-node 'src/index.ts'",
    "test": "jest"
  },
  "author": "cmcahoon@gmail.com",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^22.1.3",
    "jest": "^22.4.2",
    "ts-jest": "^22.4.0",
    "ts-node": "^5.0.0",
    "tslint": "^5.9.1",
    "typescript": "^2.7.2"
  },
  "dependencies": {
    "immutable": "^3.8.2",
    "redux": "^3.7.2"
  },
  "jest": {
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(ts)$",
    "moduleFileExtensions": [
      "ts",
      "js",
      "json"
    ]
  }
}
